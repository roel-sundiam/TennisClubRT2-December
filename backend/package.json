{
  "name": "tennis-club-backend",
  "version": "1.0.0",
  "description": "Tennis Club RT2 Backend API - December Updates",
  "main": "dist/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/roel-sundiam/TennisClubRT2-December.git"
  },
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "create-superadmin": "npx ts-node src/scripts/createSuperAdmin.ts",
    "import-members": "npx ts-node src/scripts/importMembers.ts",
    "setup-chat": "npx ts-node src/scripts/setupChatRooms.ts",
    "create-overdue-test": "npx ts-node src/scripts/createOverduePayment.ts",
    "clean-test-data": "npx ts-node src/scripts/cleanTestData.ts",
    "clear-data": "npx ts-node scripts/clear-data.ts",
    "migrate-court-usage": "node scripts/migrateCourtUsageData.js",
    "migrate-court-usage-force": "node scripts/migrateCourtUsageData.js --force",
    "backup": "npx ts-node src/scripts/backupDatabase.ts",
    "backup:compress": "npx ts-node src/scripts/backupDatabase.ts -- --compress",
    "backup:collections": "npx ts-node src/scripts/backupDatabase.ts -- --collections",
    "restore": "npx ts-node src/scripts/restoreDatabase.ts",
    "restore:dry-run": "npx ts-node src/scripts/restoreDatabase.ts -- --dry-run",
    "cleanup-orphaned-payments": "npx ts-node src/scripts/cleanupOrphanedPayments.ts",
    "test-supabase": "npx ts-node src/scripts/test-supabase-connection.ts",
    "clear-reservations-payments": "npx ts-node src/scripts/clearReservationsAndPayments.ts",
    "replicate-to-test": "npx ts-node src/scripts/replicateToTestDatabase.ts",
    "migrate-players": "npx ts-node src/scripts/migrateUsersToPlayers.ts",
    "migrate-players:rollback": "npx ts-node src/scripts/migrateUsersToPlayers.ts -- --rollback",
    "audit-tournament-data": "npx ts-node src/scripts/auditTournamentData.ts",
    "cleanup-duplicate-points": "npx ts-node src/scripts/cleanupDuplicatePoints.ts",
    "create-tournament-indexes": "npx ts-node src/scripts/createTournamentIndexes.ts",
    "test-tournament-processing": "npx ts-node src/scripts/testTournamentProcessing.ts",
    "copy-to-test": "npx ts-node src/scripts/copyDatabaseToTest.ts",
    "copy-to-test:dry-run": "npx ts-node src/scripts/copyDatabaseToTest.ts -- --dry-run",
    "copy-to-test:force": "npx ts-node src/scripts/copyDatabaseToTest.ts -- --force",
    "copy-to-test:no-tournaments": "npx ts-node src/scripts/copyDatabaseToTest.ts -- --exclude tournaments",
    "migrate-tournaments-completed": "npx ts-node src/scripts/migrateTournamentsToCompleted.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.88.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.14.12",
    "@types/sharp": "^0.31.1",
    "@types/socket.io": "^3.0.1",
    "@types/web-push": "^3.6.4",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.1.0",
    "googleapis": "^159.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.5.2",
    "multer": "^2.0.2",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "typescript": "^5.5.4",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
